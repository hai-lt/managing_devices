        <th>Name</th>
      <% end %>
      <th>Device</th>
      <th>Reason</th>
      <th>From</th>
      <th>To</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: 'registers/register', collection: @register_devices, as: :register %>
  </tbody>

</table>
