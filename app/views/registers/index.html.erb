<div class="index_table row col-sm-10 col-sm-offset-1">
  <table class="table table-striped">
    <thead>
      <tr>
        <% if @current_user.admin? %>
        <th>Name</th>
        <% end %>
        <th>Device</th>
        <th>Reason</th>
        <th>From</th>
        <th>To</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: 'registers/register', collection: @register_devices, as: :register %>
    </tbody>

  </table>
  <% render 'modals/simple_modal' %>
</div>
